# URL backend Laravel
API_BASE_URL=http://127.0.0.1:8000/api

# Token Laravel jika memakai Sanctum/JWT
API_TOKEN=your_token_here

# Path model YOLO
YOLO_MODEL=models/lp_detector.pt

# Folder input gambar dari kamera
IMAGE_INPUT=images

# Folder output crop plat
CROPPED_OUTPUT=cropped

# Interval scanning kamera (ms)
SCAN_INTERVAL=500

# ID device (jika banyak ESP32)
DEVICE_ID=GATE_01

# Google Vision API (true/false)
GOOGLE_VISION=true